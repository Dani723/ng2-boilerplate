{
  "name": "ng2 boilerplate",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "dev": "npm-run-all dev:vcl --parallel dev:serve dev:styles",
    "dev:serve": "webpack-dev-server --config webpack.config.dev.js --env.ENV development",
    "dev:vcl": "vcl-preprocessor app/styles/index.styl | postcss --use autoprefixer -o app/app.css",
    "dev:styles": "fsmonitor -d 'app/styles' npm run dev:vcl",
    "build:webpack": "webpack --env.ENV production",
    "build:vcl": "vcl-preprocessor app/styles/index.styl | postcss --use autoprefixer | cssnano > app/app.css",
    "build:lint": "tslint app/src/**/*.ts",
    "build:clean": "rimraf dist",
    "build:serve": "browser-sync -s dist start",
    "build": "npm-run-all build:lint build:clean build:vcl build:webpack",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@angular/common": "^2.0.0-rc.6",
    "@angular/compiler": "^2.0.0-rc.6",
    "@angular/compiler-cli": "^0.6.0",
    "@angular/core": "^2.0.0-rc.6",
    "@angular/forms": "2.0.0-rc.6",
    "@angular/http": "^2.0.0-rc.6",
    "@angular/platform-browser": "^2.0.0-rc.6",
    "@angular/platform-browser-dynamic": "^2.0.0-rc.6",
    "@angular/platform-server": "^2.0.0-rc.6",
    "@angular/router": "^3.0.0-rc.2",
    "core-js": "^2.4.1",
    "ng-vcl": "github:ng-vcl/ng-vcl",
    "rxjs": "5.0.0-beta.11",
    "zone.js": "0.6.17"
  },
  "devDependencies": {
    "@types/core-js": "^0.9.32",
    "@types/requirejs": "^2.1.26",
    "angular2-template-loader": "^0.5.0",
    "appcache-webpack-plugin": "^1.3.0",
    "awesome-typescript-loader": "^2.2.1",
    "copy-webpack-plugin": "^3.0.1",
    "css-loader": "^0.24.0",
    "cssnano-cli": "^1.0.4",
    "extract-text-webpack-plugin": "^2.0.0-beta.3",
    "fsmonitor": "^0.2.4",
    "html-webpack-plugin": "^2.22.0",
    "npm-run-all": "^3.1.0",
    "postcss-cli": "^2.6.0",
    "raw-loader": "^0.5.1",
    "rimraf": "^2.5.4",
    "style-loader": "^0.13.1",
    "tslint": "^3.15.1",
    "typescript": "^2.0.2",
    "vcl-core-modules": "^0.1.1",
    "vcl-default-theme": "^0.1.1",
    "vcl-default-theme-terms": "^0.1.0",
    "vcl-font-awesome": "^0.1.0",
    "vcl-preprocessor": "^0.1.1",
    "webpack": "^2.1.0-beta.19",
    "webpack-dev-server": "^2.1.0-beta.2",
    "webpack-merge": "^0.14.1"
  }
}
